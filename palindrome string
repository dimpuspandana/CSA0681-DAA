def first_palindromic_string(words):
    for word in words:
        if word == word[::-1]:
            return word
    return ""

# Example 1
words1 = ["abc", "car", "ada", "racecar", "cool"]
print(first_palindromic_string(words1))  # Output: "ada"

# Example 2
words2 = ["notapalindrome", "racecar"]
print(first_palindromic_string(words2))  # Output: "racecar"
